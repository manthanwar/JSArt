{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ "eslint:recommended", "standard" ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": [ "error", "never" ],
        "semi": [ "error", "always" ],
        "quotes": [ "error", "single" ],
        "space-before-function-paren": [ "error", "never" ]
    }
}
